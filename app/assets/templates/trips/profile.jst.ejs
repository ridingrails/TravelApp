<div class="col-xs-12 trip-show">
	<div class="row">
		<div class="col-xs-10 col-xs-offset-2">
		  <h4><%= trip.escape('title') %></h4>
		  <h4><%= trip.escape('theme') %></h4>
		</div>
	</div>
	<div class="row">
		<div class="col-xs-10 col-xs-offset-2">
		  <h4>From: <%= trip.escape('start_date') %></h4>
		  <h4>To: <%= trip.escape('end_date') %></h4>
		</div>
	</div>
	<div class="row">
		<div class="col-xs-10 col-xs-offset-2">
		  <h4>Destination: <%= trip.escape('end_loc') %></h4>
		</div>
	</div>
	<br>
	<br>
	<div class="row">
		<div class="col-xs-8 col-xs-offset-2 trip-details" id="trip-details" data-lat="<%= trip.escape('lat') %>" data-lng="<%= trip.escape('lng') %>">
			<div class="row">
				<div class="col-xs-8">
					<input type="text" id="dest-search" name="dest-search">
				</div>
				<div class="col-xs-4">
					<button class="btn btn-primary dest-search">Explore <%= trip.escape('end_loc') %> </button>
				</div>
			</div>
			<div id="map-canvas"></div>
		</div>
		<div class="col-xs-2">
		  <ul id="photo-panel">
		    <li><strong>Photos clicked</strong></li>
		  </ul>
		</div>
  </div>
</div>

