
<div class="col-xs-6">
	<h3>Sign Up</h3>
  <form id="new_user_form" method="post">
		<div class="col-xs-12">
			<div class="form-group">
			  <label for="user_email" class="col-xs-6 control-label">Email</label>
				<div class="col-xs-6">
			    <input type="text" class="form-control" id="user_email" name="user[email]">
				</div>
			</div>
		</div>
		<div class="col-xs-12">
			<br>
			<div class="form-group">
			  <label for="user_password" class="col-xs-6 control-label">Password</label>
				<div class="col-xs-6">
			    <input type="password" class="form-control" id="user_username" name="user[password]">
				</div>
				<br>
			  <input type="submit" class="btn btn-default" value="Sign Up">
			</div>
		</div>
	</form>
	<div class="fb-login-button" show-faces="true" width="200" max-rows="1"></div>
</div>



