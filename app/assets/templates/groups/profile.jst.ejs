<div class="col-xs-12 group-show">
	<div class="panel panel-default">
	  <div class="panel-body">
			<div class="row">
				<div class="col-xs-10">
				  <h4><%= group.escape('title') %></h4>
				  <h4><%= group.escape('theme') %></h4>
				  <h4><%= group.escape('description') %></h4>
				</div>
			</div>
		</div>
	</div>
	<br>
	<br>
	<div class="row">
	</div>
		<div class="col-xs-4">
			<div class="info-area">
				<div class="info-area-ul">
					<div class="panel-group" id="accordion">
						<% group.get('members').each(function(member) { %>
					  <div class="panel panel-default">
					    <div class="panel-heading">
					      <h4 class="panel-title">
					        <a data-toggle="collapse" data-parent="#accordion" href="<%= member.get('id') %>">
					          <%= member.escape('email') %>
					        </a>
					      </h4>
					    </div>
					    <div id="<%= member.get('id') %>" class="panel-collapse collapse in">
					      <div class="panel-body">
									<p><%= member.escape('name') || "" %></p>
					      </div>
					    </div>
					  </div>
						<% }); %>
					</div>
				</div>
			</div>
	  </div>
	</div>
	<div>
</div>

<div class="panel panel-default">
  <div class="panel-body">
    Basic panel example
  </div>
</div>

